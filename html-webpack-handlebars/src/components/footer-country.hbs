<div class="country">
  <div class="country-logo">
    <img
      width="225"
      height="225"
      src="{{this.src}}"
      alt=""
      loading="lazy"
      srcset="{{item.srcset}}"
      sizes="(max-width: 225px) 100vw, 225px"
    />
  </div>
  <div class="country-title">
    <h3>
      <strong>{{this.title}}</strong>
    </h3>
  </div>
  <div class="country-address">
    <p>
      {{#each this.address}}
        <br />
        {{this}}
      {{/each}}
    </p>
  </div>
</div>